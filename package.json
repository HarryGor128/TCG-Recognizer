{
    "name": "TCG_Recognizer",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start --experimental-debugger",
        "test": "jest",
        "buildAndroidWin": "cd android && gradlew assembleRelease && cd ..",
        "buildAndroidMac": "cd android && ./gradlew assembleRelease && cd ..",
        "cleanAndroidWin": "cd android && gradlew clean && cd ..",
        "cleanAndroidMac": "cd android && ./gradlew clean && cd ..",
        "portRestart": "net stop winnat && net start winnat",
        "reinstall": "rm -rf node_modules && rm package-lock.json && npm install",
        "reset": "npm start -- --reset-cache",
        "adbReverse": "adb reverse tcp:8081 tcp:8081",
        "adbRestart": "adb kill-server && adb start-server",
        "stopGradleWin": "cd android && gradlew --stop && cd ..",
        "stopGradleMac": "cd android && ./gradlew --stop && cd ..",
        "wifiConnect": "adb connect 192.168.1.9:40697",
        "wifiDisconnect": "adb disconnect 192.168.1.9:40697"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-brands-svg-icons": "^6.4.2",
        "@fortawesome/free-regular-svg-icons": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@fortawesome/react-native-fontawesome": "^0.3.0",
        "@react-native-async-storage/async-storage": "^1.23.1",
        "@react-navigation/native": "^6.1.7",
        "@react-navigation/native-stack": "^6.9.13",
        "@react-navigation/stack": "^6.3.17",
        "@reduxjs/toolkit": "^1.9.5",
        "@viro-community/react-viro": "^2.41.1",
        "axios": "^1.5.0",
        "i18next": "^23.8.1",
        "moment": "^2.29.4",
        "node-html-parser": "^6.1.12",
        "patch-package": "^8.0.0",
        "react": "18.2.0",
        "react-i18next": "^14.0.1",
        "react-native": "0.73.6",
        "react-native-gesture-handler": "^2.12.1",
        "react-native-image-picker": "^7.0.0",
        "react-native-permissions": "^3.9.2",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-screens": "^3.25.0",
        "react-native-svg": "^13.13.0",
        "react-redux": "^8.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/babel-preset": "0.73.21",
        "@react-native/eslint-config": "0.73.2",
        "@react-native/metro-config": "0.73.5",
        "@react-native/typescript-config": "0.73.1",
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/react": "^18.2.6",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.6.3",
        "eslint": "^8.19.0",
        "jest": "^29.6.3",
        "prettier": "2.8.8",
        "react-test-renderer": "18.2.0",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">=18"
    }
}
